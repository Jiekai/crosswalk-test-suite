          "install-path": "testapp"
        },
        "testapp/loadimagelist": {
          "apk-type": "MANIFEST",
          "install-path": "testapp"
        },
        "testapp/scrolllargehightpage": {
          "apk-type": "MANIFEST",
          "install-path": "testapp"
        }
      }
    },
    "apk-aio": {
      "blacklist": []
    }
  },
  "pkg-name": "stability-manual-android-tests"
}
