        "sysmon-chrome.sh": "sysmon-chrome.sh",
        "testapp/README.md": "testapp/README.md",
        "tests.full.xml": "tests.full.xml",
        "tests.xml": "tests.xml",
        "testscripts": "testscripts"
      },
      "pkg-app": {
        "apk-type": "MANIFEST",
        "copylist": {
          "PACK-TOOL-ROOT/resources/webrunner": "webrunner"
        }
      },
      "subapp-list": {
        "testapp/crossfuzz": {
          "apk-type": "MANIFEST",
          "install-path": "testapp"
        },
        "testapp/downloadtest": {
