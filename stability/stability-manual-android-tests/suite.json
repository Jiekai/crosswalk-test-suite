{
  "pkg-blacklist": [
    "pack.py",
    "testcase.xsl",
    "testresult.xsl",
    "tests.css",
    "icon.png",
    "suite.json",
    "inst.*"
  ],
  "pkg-list": {
    "apk,cordova": {
      "blacklist": [
        "*"
      ],
