<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite category="W3C/HTML5 APIs" name="webapi-hrtime-w3c-tests">
    <set name="hrtime" type="js">
      <testcase component="W3C_HTML5 APIs/Performance and Optimization/High Resolution Time" execution_type="auto" id="monotonic-clock" priority="P1" purpose="Test Performance monotonic clock" status="approved" type="compliance" subcase="2">
        <description>
          <test_script_entry>/opt/webapi-hrtime-w3c-tests/hrtime/w3c/monotonic-clock.html</test_script_entry>
        </description>
        <specs>
          <spec>
            <spec_assertion category="W3C API Specifications" element_name="now" element_type="method" interface="Performance" section="Performance and Optimization" specification="High Resolution Time" />
            <spec_url>http://www.w3.org/TR/hr-time/#sec-extenstions-performance-interface</spec_url>
            <spec_statement />
          </spec>
        </specs>
      </testcase>
      <testcase component="W3C_HTML5 APIs/Performance and Optimization/High Resolution Time" execution_type="auto" id="test_cross_frame_start" onload_delay="5" priority="P1" purpose="Test Performance cross frame start" status="approved" type="compliance" subcase="4">
        <description>
          <test_script_entry>/opt/webapi-hrtime-w3c-tests/hrtime/w3c/test_cross_frame_start.html</test_script_entry>
        </description>
        <specs>
          <spec>
            <spec_assertion category="W3C API Specifications" element_name="now" element_type="method" interface="Performance" section="Performance and Optimization" specification="High Resolution Time" />
            <spec_url>http://www.w3.org/TR/hr-time/#sec-extenstions-performance-interface</spec_url>
            <spec_statement />
          </spec>
        </specs>
      </testcase>
    </set>
  </suite>
</test_definition>
