<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite category="W3C/HTML5 APIs" name="tct-touchevent-w3c-tests">
    <set name="TouchEvent" type="js">
      <testcase component="W3C_HTML5 APIs/Device/Touch Events version 1" execution_type="auto" id="document_createTouch_exist" purpose="Check if document.createtouch method is defined.">
        <description>
          <test_script_entry>/opt/tct-touchevent-w3c-tests/touchevent/document_createTouch_exist.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="W3C_HTML5 APIs/Device/Touch Events version 1" execution_type="auto" id="document_createTouchList_exist" purpose="Check if document.createtouchlist method is defined.">
        <description>
          <test_script_entry>/opt/tct-touchevent-w3c-tests/touchevent/document_createTouchList_exist.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="W3C_HTML5 APIs/Device/Touch Events version 1" execution_type="manual" id="multi-touch-interactions" purpose="Test multi touch interactions">
        <description>
          <pre_condition>Test device supports touch.</pre_condition>
          <steps>
            <step order="1">
              <step_desc>Open the test page: multi-touch-interactions-manual.html</step_desc>
              <expected>Page opened with 2 touch boxes</expected>
            </step>
            <step order="2">
              <step_desc>Follow the instructions on the page</step_desc>
              <expected>Pass</expected>
            </step>
          </steps>
          <test_script_entry test_script_expected_result="0">/opt/tct-touchevent-w3c-tests/touchevent/w3c/multi-touch-interactions-manual.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="W3C_HTML5 APIs/Device/Touch Events version 1" execution_type="manual" id="single-touch" purpose="Test single touch interactions">
        <description>
          <pre_condition>Test device supports touch.</pre_condition>
          <steps>
            <step order="1">
              <step_desc>Open the test page: single-touch-manual.html</step_desc>
              <expected>Page opened with 2 touch boxes</expected>
            </step>
            <step order="2">
              <step_desc>Follow the instructions on the page</step_desc>
              <expected>Pass</expected>
            </step>
          </steps>
          <test_script_entry test_script_expected_result="0">/opt/tct-touchevent-w3c-tests/touchevent/w3c/single-touch-manual.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="W3C_HTML5 APIs/Device/Touch Events version 1" execution_type="auto" id="create-touch-touchlist" purpose="Test createTouch creates Touch object with requested properties" subcase="73">
        <description>
          <test_script_entry>/opt/tct-touchevent-w3c-tests/touchevent/w3c/create-touch-touchlist.html</test_script_entry>
        </description>
      </testcase>
    </set>
    <set name="TouchEvent-bdd" type="js" ui-auto="bdd">
      <testcase component="W3C_HTML5 APIs/Device/Touch Events version 1" execution_type="manual" id="TouchEvent_altKey_exist" purpose="Check if touchEvent.altKey attribute is defined.">
        <description>
          <steps>
            <step order="1">
              <step_desc>Use WRT to visit the "touchEvent_touchEvent_altKey" Check if touchEvent.altKey attribute is defined.</step_desc>
              <expected>Pass</expected>
            </step>
          </steps>
          <test_script_entry>/opt/tct-touchevent-w3c-tests/touchevent/bdd/TouchEvent_altKey_exist-manual.html</test_script_entry>
          <bdd_test_script_entry test_script_expected_result="0">/opt/tct-touchevent-w3c-tests/w3c-touchevent-app/testscripts/TouchEvent_altKey_exist.feature</bdd_test_script_entry>
        </description>
      </testcase>
      <testcase component="W3C_HTML5 APIs/Device/Touch Events version 1" execution_type="manual" id="TouchEvent_ctrlKey_exist" purpose="Check if touchEvent.ctrlKey attribute is defined.">
