<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite category="Cordova Sample Apps" name="cordova-sampleapp-android-tests">
    <set name="SampleApp_auto" type="pyunit">
      <testcase component="Cordova Sample Apps/Gallery" execution_type="auto" id="Crosswalk_Cordova_SampleApp_Gallery_Build" purpose="Validate if sample app Gallery could be built successfully">
        <description>
          <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/gallery_build.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/Gallery" execution_type="auto" id="Crosswalk_Cordova_SampleApp_Gallery_Install" purpose="Validate if sample app Gallery could be installed successfully">
        <description>
          <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/gallery_install.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/Gallery" execution_type="auto" id="Crosswalk_Cordova_SampleApp_Gallery_Launch" purpose="Validate if sample app Gallery could be launched successfully">
        <description>
           <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/gallery_launch.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/Gallery" execution_type="auto" id="Crosswalk_Cordova_SampleApp_Gallery_Close" purpose="Validate if sample app Gallery could be closed successfully">
        <description>
          <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/gallery_close.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/Gallery" execution_type="auto" id="Crosswalk_Cordova_SampleApp_Gallery_UnInstall" purpose="Validate if sample app Gallery could be uninstalled successfully">
        <description>
          <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/gallery_uninstall.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/Gallery" execution_type="auto" id="Crosswalk_Cordova_SampleApp_Gallery_Uninstall_WithAppRunning" purpose="Validate Crosswalk support that uninstall the sample app Gallery when it's actually running">
        <description>
           <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/gallery_uninstall_withAppRunning.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/CIRC" execution_type="auto" id="CIRC_build" purpose="Validate if sample app CIRC could be built successfully">
        <description>
           <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/CIRC_build.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/CIRC" execution_type="auto" id="CIRC_install" purpose="Validate if sample app CIRC could be installed successfully">
        <description>
           <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/CIRC_install.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/CIRC" execution_type="auto" id="CIRC_launch" purpose="Validate if sample app CIRC could be launched successfully">
        <description>
           <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/CIRC_launch.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/CIRC" execution_type="auto" id="CIRC_close" purpose="Validate if sample app CIRC could be closed successfully">
        <description>
           <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/CIRC_close.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/CIRC" execution_type="auto" id="CIRC_uninstall" purpose="Validate if sample app CIRC could be uninstalled successfully">
        <description>
           <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/CIRC_uninstall.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/Eh" execution_type="auto" id="Eh_build" purpose="Validate if sample app Eh could be built successfully">
        <description>
           <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/Eh_build.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/Eh" execution_type="auto" id="Eh_install" purpose="Validate if sample app Eh could be installed successfully">
        <description>
           <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/Eh_install.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/Eh" execution_type="auto" id="Eh_launch" purpose="Validate if sample app Eh could be launched successfully">
        <description>
           <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/Eh_launch.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/Eh" execution_type="auto" id="Eh_close" purpose="Validate if sample app Eh could be closed successfully">
        <description>
           <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/Eh_close.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/Eh" execution_type="auto" id="Eh_uninstall" purpose="Validate if sample app Eh could be uninstalled successfully">
        <description>
           <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/Eh_uninstall.py</test_script_entry>
        </description>
      </testcase>
    </set>
    <set name="SampleApp_manual" type="js">
      <testcase component="Cordova Sample Apps/Gallery" execution_type="manual" id="Crosswalk_Cordova_SampleApp_Gallery_AppFunction" purpose="Validate if sample app Gallery function could work well">
        <description>
          <pre_condition>
            1. Make sure the Gallery webapp is launched.
          </pre_condition>
          <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/Crosswalk_Cordova_SampleApp_Gallery_AppFunction.html </test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/Gallery" execution_type="manual" id="Crosswalk_Cordova_SampleApp_Gallery_Icon" purpose="Validate if sample app Gallery icon display normally">
        <description>
          <pre_condition>
            1. Make sure Gallery webapp is installed;
            2. Make sure Gallery webapp is available.
          </pre_condition>
          <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/Crosswalk_Cordova_SampleApp_Gallery_Icon.html </test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/Gallery" execution_type="manual" id="Crosswalk_Cordova_SampleApp_Gallery_Switch" purpose="Validate if sample app Gallery could switch back successfully">
        <description>
          <pre_condition>
            1. Make sure the Gallery webapp is launched.
          </pre_condition>
          <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/Crosswalk_Cordova_SampleApp_Gallery_Switch.html </test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/Gallery" execution_type="manual" id="Crosswalk_Cordova_SampleApp_Gallery_Orientation" purpose="Validate if sample app Gallery could rotate between portrait orientation screen and landscape orientation screen">
        <description>
          <pre_condition>
            1. Make sure the Gallery webapp is launched.
          </pre_condition>
          <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/Crosswalk_Cordova_SampleApp_Gallery_Orientation.html </test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/Gallery" execution_type="manual" id="Crosswalk_Cordova_SampleApp_Gallery_PresentationFunction" purpose="Validate if sample app Gallery Presentation function could work well">
        <description>
          <pre_condition>
            1. Make sure the Gallery webapp is launched.
          </pre_condition>
          <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/Crosswalk_Cordova_SampleApp_Gallery_PresentationFunction.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/CIRC" execution_type="manual" id="Crosswalk_Cordova_SampleApp_CIRC_Chat" purpose="Validate if sample app Circ basic chat function could work well">
        <description>
          <pre_condition>
            1. Make sure the Circ webapp is launched in two adnroid devices.
          </pre_condition>
          <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/Crosswalk_Cordova_SampleApp_CIRC_Chat.html </test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/CIRC" execution_type="manual" id="Crosswalk_Cordova_SampleApp_CIRC_Channel" purpose="Validate if sample app Circ basic channel function could work well">
        <description>
          <pre_condition>
            1. Make sure the Circ webapp is launched in two adnroid devices.
          </pre_condition>
          <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/Crosswalk_Cordova_SampleApp_CIRC_Channel.html </test_script_entry>
        </description>
      </testcase>
      <testcase component="Cordova Sample Apps/Eh" execution_type="manual" id="Crosswalk_Cordova_SampleApp_Eh_AppFunction" purpose="Validate if sample app Eh basic function could work well">
        <description>
          <pre_condition>
            1. Make sure the Eh webapp is installed in two adnroid devices.
          </pre_condition>
          <test_script_entry>/opt/cordova-sampleapp-android-tests/sampleapp/Crosswalk_Cordova_SampleApp_Eh_AppFunction.html </test_script_entry>
        </description>
      </testcase>
    </set>
  </suite>
</test_definition>
